<%= simple_form_for @driver_profile do |f| %>
  <%= f.input :title %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :address %>
  <%= f.input :zip_code %>
  <%= f.input :city %>
  <%= f.input :birthdate %>
  <%= f.input :telephone_number %>
  <%= f.input :driving_license %>
  <%= f.input :identity_card %>
  <%= f.button :submit %>
<% end %>
